services:
  basket.api:
    build:
      context: D:\SamplesAPI\aspnet-microservices
      dockerfile: Services/Basket/Basket.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Basket.Api
      target: base
    container_name: basket.api
    depends_on:
      basketdb:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '8004'
      ASPNETCORE_Kestrel__Certificates__Default__Password: 123456-a
      ASPNETCORE_Kestrel__Certificates__Default__Path: /https/aspnetapp.pfx
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443
      CacheSettings:ConnectionString: basketdb:6379
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      GrpcSettings:DiscountUrl: http://discount.grpc
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: basketapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8004
      target: 443
    tty: true
    volumes:
    - D:\SamplesAPI\aspnet-microservices\Services\Basket\Basket.Api:/app:rw
    - C:\Users\RICARDO\.aspnet\https:/https:ro
    - C:\Users\RICARDO\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\RICARDO\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\RICARDO\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\RICARDO\.nuget\packages:/root/.nuget/packages:ro
    - D:\SamplesAPI\aspnet-microservices:/src:rw
  basketdb:
    container_name: basketdb
    image: redis:alpine
    ports:
    - published: 6379
      target: 6379
    restart: always
  catalog.api:
    build:
      context: D:\SamplesAPI\aspnet-microservices
      dockerfile: Services/Catalog/Catalog.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Catalog.Api
      target: base
    container_name: catalog.api
    depends_on:
      catalogdb:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '8002'
      ASPNETCORE_Kestrel__Certificates__Default__Password: 123456-a
      ASPNETCORE_Kestrel__Certificates__Default__Path: /https/aspnetapp.pfx
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      DatabaseSettings:ConnectionString: mongodb://catalogdb:27017
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/Catalog.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8002
      target: 443
    tty: true
    volumes:
    - D:\SamplesAPI\aspnet-microservices\Services\Catalog\Catalog.Api:/app:rw
    - C:\Users\RICARDO\.aspnet\https:/https:ro
    - C:\Users\RICARDO\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\RICARDO\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\RICARDO\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\RICARDO\.nuget\packages:/root/.nuget/packages:ro
    - D:\SamplesAPI\aspnet-microservices:/src:rw
  catalogdb:
    container_name: catalogdb
    image: mongo
    ports:
    - published: 27017
      target: 27017
    restart: always
    volumes:
    - mongo_data:/data/db:rw
  discount.api:
    build:
      context: D:\SamplesAPI\aspnet-microservices
      dockerfile: Services/Discount/Discount.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Discount.Api
      target: base
    container_name: discount.api
    depends_on:
      discountdb:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '8006'
      ASPNETCORE_Kestrel__Certificates__Default__Password: 123456-a
      ASPNETCORE_Kestrel__Certificates__Default__Path: /https/aspnetapp.pfx
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      DatabaseSettings:ConnectionString: Server=discountdb;Port=5432;Database=DiscountDb;User
        Id=admin;Password=admin1234;
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: discountapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8006
      target: 443
    tty: true
    volumes:
    - D:\SamplesAPI\aspnet-microservices\Services\Discount\Discount.Api:/app:rw
    - C:\Users\RICARDO\.aspnet\https:/https:ro
    - C:\Users\RICARDO\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\RICARDO\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\RICARDO\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\RICARDO\.nuget\packages:/root/.nuget/packages:ro
    - D:\SamplesAPI\aspnet-microservices:/src:rw
  discount.grpc:
    build:
      context: D:\SamplesAPI\aspnet-microservices
      dockerfile: Services/Discount/Discount.Grpc/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Discount.Grpc
      target: base
    container_name: discount.grpc
    depends_on:
      discountdb:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORT: '8007'
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      DatabaseSettings:ConnectionString: Server=discountdb;Port=5432;Database=DiscountDb;User
        Id=admin;Password=admin1234;
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: discountgrpc:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8007
      target: 80
    tty: true
    volumes:
    - D:\SamplesAPI\aspnet-microservices\Services\Discount\Discount.Grpc:/app:rw
    - C:\Users\RICARDO\.aspnet\https:/https:ro
    - C:\Users\RICARDO\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\RICARDO\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\RICARDO\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\RICARDO\.nuget\packages:/root/.nuget/packages:ro
    - D:\SamplesAPI\aspnet-microservices:/src:rw
  discountdb:
    container_name: discountdb
    environment:
      POSTGRES_DB: DiscountDb
      POSTGRES_PASSWORD: admin1234
      POSTGRES_USER: admin
    image: postgres
    ports:
    - published: 5432
      target: 5432
    restart: always
    volumes:
    - postgres_data:/var/lib/postgresql/data/:rw
  orderdb:
    container_name: orderdb
    environment:
      ACCEPT_EULA: "y"
      SA_PASSWORD: SwN12345678
    image: mcr.microsoft.com/mssql/server:2017-latest
    ports:
    - published: 1466
      target: 1433
    restart: always
  pgadmin:
    container_name: pgadmin
    environment:
      PGADMIN_DEFAULT_EMAIL: admin@aspnet.com
      PGADMIN_DEFAULT_PASSWORD: admin1234
    image: dpage/pgadmin4
    ports:
    - published: 5050
      target: 80
    restart: always
    volumes:
    - pgadmin_data:/root/.pgadmin:rw
  portainer:
    container_name: portainer
    image: portainer/portainer-ce
    ports:
    - published: 8000
      target: 8000
    - published: 9000
      target: 9000
    restart: always
    volumes:
    - portainer_data:/data:rw
    - /var/run/docker.sock:/var/run/docker.sock:rw
version: '3.4'
volumes:
  mongo_data: {}
  pgadmin_data: {}
  portainer_data: {}
  postgres_data: {}